{
  "name": "stack",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "node scripts/build.js --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "build": "node scripts/build.js",
    "start": "node dist/server.js",
