  },
  "dependencies": {
    "@fontsource/manrope": "^5.2.6",
    "better-sqlite3": "^12.2.0",
    "express": "^4.21.2",
    "lucide-preact": "^0.525.0",
    "pino": "^9.7.0",
    "pino-http": "^10.5.0",
    "preact": "^10.24.3",
    "preact-render-to-string": "^6.5.11",
    "source-map-support": "^0.5.21",
    "wouter-preact": "^3.7.1",
    "yaml": "^2.8.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@playwright/test": "^1.48.2",
    "esbuild": "^0.25.8",
    "eslint": "^9.32.0",
    "eslint-plugin-react": "^7.37.5",
    "prettier": "^3.6.2"
  },
  "engines": {
    "node": ">=20.11.0",
    "npm": ">=10.0.0"
  }
